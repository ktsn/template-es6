{
  "name": "template-es6",
  "version": "1.0.0",
  "author": "katashin",
  "private": true,
  "description": "A project template for an ES6 library",
  "keywords": [
    "template",
    "ECMAScript"
  ],
  "license": "MIT",
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/template-es6",
  "bugs": "https://github.com/ktsn/template-es6/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/template-es6.git"
  },
  "scripts": {
    "prepublish": "npm run release",
    "clean": "rm -rf dist .tmp",
    "build": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:test": "webpack --config webpack.config.test.js",
    "watch:test": "webpack -w --config webpack.config.test.js",
    "flow": "flow check",
    "lint": "eslint \"src/**/*.js\" && eslint \"test/**/*.js\"",
    "testem": "testem",
    "testem:ci": " testem ci --launch Firefox,PhantomJS",
    "test": "run-p watch:test testem",
    "test:ci": "run-s build:test testem:ci",
    "release": "run-s lint flow test:ci clean build build:prod"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "eslint": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "flow-bin": "^0.29.0",
    "glob": "^7.0.3",
    "json-loader": "^0.5.4",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.3.1",
    "testem": "^1.6.0",
    "webpack": "^1.13.0"
  }
}
